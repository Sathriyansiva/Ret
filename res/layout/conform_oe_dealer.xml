<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:background="@color/toolbacolorr"
        android:fitsSystemWindows="true"
        android:gravity="center"

        android:orientation="horizontal">

        <ImageView
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:background="@drawable/tvswhlogo"

            android:gravity="center" />

        <!--  <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/toolbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/toolbacolorr"

            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            app:title=""
            app:titleTextColor="#fcfcfc"

            />-->
    </LinearLayout>

    <TextView
        android:id="@+id/profile"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/activity_text_heightlayout"
        android:layout_alignParentTop="true"
        android:background="@drawable/background_gradiant"
        android:gravity="center"
        android:text="Confirmation"
        android:textColor="@color/black"
        android:textSize="@dimen/title_textSize"
        android:typeface="normal"
        android:visibility="gone" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.1dp"
        android:layout_below="@+id/profile"
        android:background="@color/grey" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:elevation="4dp"
        android:fadingEdge="none"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:orientation="vertical"
                android:padding="5dp">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_gradiant1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:ems="10"
                        android:gravity="center"
                        android:text="Company Information"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"

                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"

                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView

                            android:layout_width="139dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"

                            android:text="OEM Dealer"
                            android:textColor="@color/start_color" />

                        <TextView
                            android:layout_width="6dp"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textColor="@color/start_color" />

                        <EditText
                            android:id="@+id/con_OEM_Dealer"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_weight="1"
                            android:cursorVisible="false"
                            android:ems="10"
                            android:focusable="false"
                            android:hint="--NILL--"
                            android:inputType="text|textCapWords"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/activity_textSize"
                            android:typeface="sans" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView

                            android:layout_width="139dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"

                            android:text="City"
                            android:textColor="@color/start_color" />

                        <TextView
                            android:layout_width="6dp"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textColor="@color/start_color" />

                        <EditText
                            android:id="@+id/con_City"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_weight="1"
                            android:cursorVisible="false"
                            android:ems="10"
                            android:focusable="false"
                            android:hint="--NILL--"
                            android:inputType="text|textCapWords"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/activity_textSize"
                            android:typeface="sans" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <TextView

                            android:layout_width="139dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"

                            android:text="Company Name"
                            android:textColor="@color/start_color" />

                        <TextView
                            android:layout_width="6dp"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textColor="@color/start_color" />

                        <EditText
                            android:id="@+id/con_Company_Name"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:cursorVisible="false"
                            android:ems="10"
                            android:focusable="false"
                            android:hint="--NILL--"
                            android:inputType="text|textCapWords"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/activity_textSize"
                            android:typeface="sans" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <TextView

                            android:layout_width="139dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"

                            android:text="Dealer Code"
                            android:textColor="@color/start_color" />

                        <TextView
                            android:layout_width="6dp"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textColor="@color/start_color" />

                        <EditText
                            android:id="@+id/con_Dealer_Code"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:cursorVisible="false"
                            android:ems="10"
                            android:focusable="false"
                            android:hint="--NILL--"
                            android:inputType="text|textCapWords"
                            android:maxLines="1"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/activity_textSize"
                            android:typeface="sans" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <TextView

                            android:layout_width="139dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"

                            android:text="Address"
                            android:textColor="@color/start_color" />

                        <TextView
                            android:layout_width="6dp"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textColor="@color/start_color" />

                        <EditText
                            android:id="@+id/con_Address"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:cursorVisible="false"
                            android:ems="10"
                            android:focusable="false"
                            android:hint="--NILL--"
                            android:inputType="text|textCapWords"
                            android:minLines="1"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/activity_textSize"
                            android:typeface="sans" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <TextView

                            android:layout_width="139dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="5dp"

                            android:text="location"
                            android:textColor="@color/start_color" />

                        <TextView
                            android:layout_width="6dp"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textColor="@color/start_color" />

                        <EditText
                            android:id="@+id/con_location"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:cursorVisible="false"
                            android:focusable="false"
                            android:hint="--NILL--"
                            android:inputType="textPostalAddress"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/activity_textSize"
                            android:typeface="sans" />


                    </LinearLayout>


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_ret_mech_elect"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="5dp">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_gradiant1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:ems="10"
                        android:gravity="center"
                        android:text=" Contact Person Information"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"

                        />
                </LinearLayout>



                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Name"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_Name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:cursorVisible="false"
                        android:ems="10"

                        android:focusable="false"
                        android:hint="--NILL--"
                        android:maxLength="10"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Phone"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_Phone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_weight="1"
                        android:cursorVisible="false"
                        android:ems="10"
                        android:focusable="false"
                        android:hint="--NILL--"
                        android:inputType="text"
                        android:textColor="@color/black"

                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/partner"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Designation"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_Designation"

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:cursorVisible="false"
                        android:ems="10"
                        android:focusable="false"
                        android:hint="--NILL--"
                        android:inputType="textPersonName"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Email"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_Email"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:cursorVisible="true"
                        android:focusable="false"

                        android:hint="--NILL--"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Were you satisfied with the customer service we provided you?"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_customer"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_weight="1"
                        android:cursorVisible="true"
                        android:ems="10"
                        android:focusable="false"
                        android:hint="--NILL--"
                        android:inputType="numberDecimal"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/product"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Comment"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_customer_Comment"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_weight="1"
                        android:cursorVisible="true"
                        android:ems="10"
                        android:focusable="false"
                        android:hint="--NILL--"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/product_other"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"

                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Was our customer service provided to you in an accessible manner?"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_accessible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_weight="1"
                        android:hint="--NILL--"
                        android:inputType="text"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_monthly"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Comment"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_accessible_Comment"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:cursorVisible="true"
                        android:ems="10"
                        android:focusable="false"

                        android:hint="--NILL--"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/major"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Did you experience any problems accessing our network or services?"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_network"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_weight="1"
                        android:cursorVisible="true"
                        android:ems="10"
                        android:focusable="false"
                        android:hint="--NILL--"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/majr_branddetlsother"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView

                        android:layout_width="139dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"

                        android:text="Comment"
                        android:textColor="@color/start_color" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:textColor="@color/start_color" />

                    <EditText
                        android:id="@+id/con_network_Comment"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_weight="1"
                        android:cursorVisible="true"
                        android:ems="10"
                        android:focusable="false"
                        android:hint="--NILL--"
                        android:textColor="@color/black"
                        android:textSize="@dimen/activity_textSize"
                        android:typeface="sans" />


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/con_register"
                    android:layout_width="@dimen/login_button_width"
                    android:layout_height="@dimen/usernameText_Height"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/admin_signintext_round"
                    android:elevation="9dp"
                    android:text="Register"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/con_cancel"
                    android:layout_width="@dimen/login_button_width"
                    android:layout_height="@dimen/usernameText_Height"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/admin_signintext_round"
                    android:elevation="9dp"
                    android:text="Cancel"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textStyle="bold" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>